{
    "compilerOptions": {
        "jsx": "react",
        "lib": ["dom","es6", "esnext"],
        "module": "esnext",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "target": "ES6"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": ["node_modules"]
}
